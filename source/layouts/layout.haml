!!! 5
%html
  %head
    %meta(charset="utf-8")
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    %meta(content="IE=edge,chrome=1" http-equiv="X-UA-Compatible")
    <!-- Use title if it's in the page YAML frontmatter -->
    %title= data.page.title || "The Middleman"
    = stylesheet_link_tag "screen"
  
  %body(class="#{page_classes}")= yield

  = javascript_include_tag  "socket.io"
  = javascript_include_tag  "jquery-1.7.2.min"
  = javascript_include_tag  "jquery.tmpl"
  = javascript_include_tag  "jquery.cookie"
  = javascript_include_tag  "knockout-2.1.0"
  = javascript_include_tag  "main"
